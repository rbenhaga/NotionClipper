{
  "name": "@notion-clipper/core-shared",
  "version": "1.0.0",
  "description": "Shared business logic (browser-safe)",
  "main": "dist/index.js",
  "module": "dist-esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist-esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "private": true,
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc",
    "build:esm": "tsc --module ESNext --moduleResolution bundler --outDir dist-esm && node scripts/build-esm.js",
    "dev": "tsc --watch",
    "clean": "rimraf dist dist-esm",
    "test": "jest"
  },
  "dependencies": {
    "@notion-clipper/notion-parser": "workspace:*",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "author": "Rayane Ben Haga",
  "license": "UNLICENSED"
}