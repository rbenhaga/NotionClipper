{
  "name": "@notion-clipper/notion-editor",
  "version": "1.0.0",
  "description": "Modular Notion-style editor component",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./styles": "./src/styles/notion-blocks.css",
    "./styles/notion-blocks.css": "./src/styles/notion-blocks.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "private": true,
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "dev": "vite build --watch",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@notion-clipper/core-shared": "workspace:*",
    "@notion-clipper/media-handlers": "workspace:*",
    "@notion-clipper/file-handlers": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.23",
    "@vitejs/plugin-react": "^4.2.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.0",
    "vite-plugin-dts": "^3.7.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "author": "Rayane Ben Haga",
  "license": "UNLICENSED"
}
