      return issues;
    } else
    /* istanbul ignore next */
    {
      cov_1uvedylfvy().b[61][1]++;
    }
    cov_1uvedylfvy().s[169]++;
    if (!block.equation.expression) {
      /* istanbul ignore next */
      cov_1uvedylfvy().b[62][0]++;
      cov_1uvedylfvy().s[170]++;
      issues.push({
        type: 'error',
