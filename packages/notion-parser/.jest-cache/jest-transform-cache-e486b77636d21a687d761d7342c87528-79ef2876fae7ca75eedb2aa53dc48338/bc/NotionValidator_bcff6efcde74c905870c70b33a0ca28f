      /* istanbul ignore next */
      {
        cov_1uvedylfvy().b[63][1]++;
      }
    }
    /* istanbul ignore next */
    cov_1uvedylfvy().s[173]++;
    return issues;
  }
  validateRichText(richText, field) {
    /* istanbul ignore next */
    cov_1uvedylfvy().f[22]++;
    const issues =
    /* istanbul ignore next */
    (cov_1uvedylfvy().s[174]++, []);
    /* istanbul ignore next */
    cov_1uvedylfvy().s[175]++;
    if (!Array.isArray(richText)) {
      /* istanbul ignore next */
      cov_1uvedylfvy().b[64][0]++;
      cov_1uvedylfvy().s[176]++;
      issues.push({
        type: 'error',
        code: 'INVALID_RICH_TEXT_FORMAT',
        message: `${field} must be an array`,
        field
      });
      /* istanbul ignore next */
      cov_1uvedylfvy().s[177]++;
      return issues;
    } else
    /* istanbul ignore next */
    {
      cov_1uvedylfvy().b[64][1]++;
    }
    // Calculate total length
    const totalLength =
    /* istanbul ignore next */
    (cov_1uvedylfvy().s[178]++, richText.reduce((sum, item) => {
      /* istanbul ignore next */
      cov_1uvedylfvy().f[23]++;
      const content =
      /* istanbul ignore next */
      (cov_1uvedylfvy().s[179]++, item.type === 'text' ?
      /* istanbul ignore next */
      (cov_1uvedylfvy().b[65][0]++,
      /* istanbul ignore next */
      (cov_1uvedylfvy().b[66][0]++, item.text?.content) ||
      /* istanbul ignore next */
      (cov_1uvedylfvy().b[66][1]++, '')) :
      /* istanbul ignore next */
      (cov_1uvedylfvy().b[65][1]++, item.type === 'equation' ?
      /* istanbul ignore next */
      (cov_1uvedylfvy().b[67][0]++,
      /* istanbul ignore next */
      (cov_1uvedylfvy().b[68][0]++, item.equation?.expression) ||
      /* istanbul ignore next */
      (cov_1uvedylfvy().b[68][1]++, '')) :
      /* istanbul ignore next */
      (cov_1uvedylfvy().b[67][1]++, '')));
      /* istanbul ignore next */
      cov_1uvedylfvy().s[180]++;
      return sum + content.length;
    }, 0));
    /* istanbul ignore next */
    cov_1uvedylfvy().s[181]++;
    if (totalLength > this.limits.maxRichTextLength) {
      /* istanbul ignore next */
      cov_1uvedylfvy().b[69][0]++;
      cov_1uvedylfvy().s[182]++;
      issues.push({
        type: 'warning',
        code: 'RICH_TEXT_TOO_LONG',
        message: `Rich text length (${totalLength}) exceeds recommended limit (${this.limits.maxRichTextLength})`,
        field
      });
    } else
    /* istanbul ignore next */
    {
      cov_1uvedylfvy().b[69][1]++;
    }
    // Validate each rich text item
    cov_1uvedylfvy().s[183]++;
    richText.forEach((item, index) => {
      /* istanbul ignore next */
      cov_1uvedylfvy().f[24]++;
      cov_1uvedylfvy().s[184]++;
      if (
      /* istanbul ignore next */
      (cov_1uvedylfvy().b[71][0]++, !item.type) ||
      /* istanbul ignore next */
      (cov_1uvedylfvy().b[71][1]++, !['text', 'mention', 'equation'].includes(item.type))) {
        /* istanbul ignore next */
        cov_1uvedylfvy().b[70][0]++;
        cov_1uvedylfvy().s[185]++;
        issues.push({
          type: 'error',
          code: 'INVALID_RICH_TEXT_TYPE',
          message: `Rich text item ${index} has invalid type: ${item.type}`,
          field: `${field}[${index}]`
        });
      } else
      /* istanbul ignore next */
      {
        cov_1uvedylfvy().b[70][1]++;
      }
      cov_1uvedylfvy().s[186]++;
      if (
      /* istanbul ignore next */
      (cov_1uvedylfvy().b[73][0]++, item.type === 'text') &&
      /* istanbul ignore next */
      (cov_1uvedylfvy().b[73][1]++, !item.text)) {
        /* istanbul ignore next */
        cov_1uvedylfvy().b[72][0]++;
        cov_1uvedylfvy().s[187]++;
        issues.push({
          type: 'error',
          code: 'MISSING_TEXT_CONTENT',
          message: `Rich text item ${index} is missing text field`,
          field: `${field}[${index}]`
        });
      } else
      /* istanbul ignore next */
      {
        cov_1uvedylfvy().b[72][1]++;
      }
      cov_1uvedylfvy().s[188]++;
      if (
      /* istanbul ignore next */
      (cov_1uvedylfvy().b[75][0]++, item.type === 'equation') &&
      /* istanbul ignore next */
      (cov_1uvedylfvy().b[75][1]++, !item.equation?.expression)) {
        /* istanbul ignore next */
        cov_1uvedylfvy().b[74][0]++;
        cov_1uvedylfvy().s[189]++;
        issues.push({
          type: 'error',
          code: 'MISSING_EQUATION_EXPRESSION',
          message: `Rich text equation ${index} is missing expression`,
          field: `${field}[${index}]`
        });
      } else
      /* istanbul ignore next */
      {
        cov_1uvedylfvy().b[74][1]++;
      }
    });
    /* istanbul ignore next */
    cov_1uvedylfvy().s[190]++;
    return issues;
  }
  validateBlockNesting(blocks, _options) {
    /* istanbul ignore next */
    cov_1uvedylfvy().f[25]++;
    const issues =
    /* istanbul ignore next */
    (cov_1uvedylfvy().s[191]++, []);
    // Check for proper list nesting
    /* istanbul ignore next */
    cov_1uvedylfvy().s[192]++;
    for (let i =
    /* istanbul ignore next */
    (cov_1uvedylfvy().s[193]++, 0); i < blocks.length; i++) {
      const block =
      /* istanbul ignore next */
      (cov_1uvedylfvy().s[194]++, blocks[i]);
      /* istanbul ignore next */
      cov_1uvedylfvy().s[195]++;
      if (this.isListItem(block)) {
        /* istanbul ignore next */
        cov_1uvedylfvy().b[76][0]++;
        // Check if list items are properly grouped
        const nextBlock =
        /* istanbul ignore next */
        (cov_1uvedylfvy().s[196]++, blocks[i + 1]);
        /* istanbul ignore next */
        cov_1uvedylfvy().s[197]++;
        if (
        /* istanbul ignore next */
        (cov_1uvedylfvy().b[78][0]++, nextBlock) &&
        /* istanbul ignore next */
        (cov_1uvedylfvy().b[78][1]++, this.isListItem(nextBlock)) &&
        /* istanbul ignore next */
        (cov_1uvedylfvy().b[78][2]++, !this.areCompatibleListItems(block, nextBlock))) {
          /* istanbul ignore next */
          cov_1uvedylfvy().b[77][0]++;
          cov_1uvedylfvy().s[198]++;
          issues.push({
            type: 'warning',
            code: 'MIXED_LIST_TYPES',
            message: `Mixed list types detected at block ${i}`,
            blockIndex: i
          });
        } else
        /* istanbul ignore next */
        {
          cov_1uvedylfvy().b[77][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_1uvedylfvy().b[76][1]++;
      }
    }
    /* istanbul ignore next */
    cov_1uvedylfvy().s[199]++;
    return issues;
  }
  isListItem(block) {
    /* istanbul ignore next */
    cov_1uvedylfvy().f[26]++;
    cov_1uvedylfvy().s[200]++;
    return ['bulleted_list_item', 'numbered_list_item', 'to_do'].includes(block.type);
  }
  areCompatibleListItems(block1, block2) {
    /* istanbul ignore next */
    cov_1uvedylfvy().f[27]++;
    cov_1uvedylfvy().s[201]++;
    // Bulleted and to-do items can be mixed
    if (
    /* istanbul ignore next */
    (cov_1uvedylfvy().b[80][0]++, block1.type === 'bulleted_list_item') &&
    /* istanbul ignore next */
    (cov_1uvedylfvy().b[80][1]++, block2.type === 'to_do') ||
    /* istanbul ignore next */
    (cov_1uvedylfvy().b[80][2]++, block1.type === 'to_do') &&
    /* istanbul ignore next */
    (cov_1uvedylfvy().b[80][3]++, block2.type === 'bulleted_list_item')) {
      /* istanbul ignore next */
      cov_1uvedylfvy().b[79][0]++;
      cov_1uvedylfvy().s[202]++;
      return true;
    } else
    /* istanbul ignore next */
    {
      cov_1uvedylfvy().b[79][1]++;
    }
    // Same types are compatible
    cov_1uvedylfvy().s[203]++;
    return block1.type === block2.type;
  }
  /**
   * Validate URLs accessibility (if enabled)
   */
  async validateUrlAccessibility(blocks) {
    /* istanbul ignore next */
    cov_1uvedylfvy().f[28]++;
    const errors =
    /* istanbul ignore next */
    (cov_1uvedylfvy().s[204]++, []);
    const warnings =
    /* istanbul ignore next */
    (cov_1uvedylfvy().s[205]++, []);
    const urls =
    /* istanbul ignore next */
    (cov_1uvedylfvy().s[206]++, this.extractUrls(blocks));
    /* istanbul ignore next */
    cov_1uvedylfvy().s[207]++;
    for (const {
      url,
      blockIndex,
      field
    } of urls) {
      /* istanbul ignore next */
      cov_1uvedylfvy().s[208]++;
      try {
        // Simple URL validation (in real implementation, you might want to make HTTP requests)
        const urlObj =
        /* istanbul ignore next */
        (cov_1uvedylfvy().s[209]++, new URL(url));
        // Check for common issues
        /* istanbul ignore next */
        cov_1uvedylfvy().s[210]++;
        if (
        /* istanbul ignore next */
        (cov_1uvedylfvy().b[82][0]++, urlObj.protocol !== 'https:') &&
        /* istanbul ignore next */
        (cov_1uvedylfvy().b[82][1]++, urlObj.protocol !== 'http:')) {
          /* istanbul ignore next */
          cov_1uvedylfvy().b[81][0]++;
          cov_1uvedylfvy().s[211]++;
          warnings.push({
            type: 'warning',
            code: 'UNSUPPORTED_PROTOCOL',
            message: `URL uses unsupported protocol: ${urlObj.protocol}`,
            blockIndex,
            field
          });
        } else
        /* istanbul ignore next */
        {
          cov_1uvedylfvy().b[81][1]++;
        }
        cov_1uvedylfvy().s[212]++;
        if (
        /* istanbul ignore next */
        (cov_1uvedylfvy().b[84][0]++, urlObj.hostname === 'localhost') ||
        /* istanbul ignore next */
        (cov_1uvedylfvy().b[84][1]++, urlObj.hostname.startsWith('127.'))) {
          /* istanbul ignore next */
          cov_1uvedylfvy().b[83][0]++;
          cov_1uvedylfvy().s[213]++;
          warnings.push({
            type: 'warning',
            code: 'LOCAL_URL',
            message: `URL points to localhost: ${url}`,
            blockIndex,
            field
          });
        } else
        /* istanbul ignore next */
        {
          cov_1uvedylfvy().b[83][1]++;
        }
      } catch (error) {
        /* istanbul ignore next */
        cov_1uvedylfvy().s[214]++;
        errors.push({
          type: 'error',
          code: 'INVALID_URL_FORMAT',
          message: `Invalid URL format: ${url}`,
          blockIndex,
          field
        });
      }
    }
    /* istanbul ignore next */
    cov_1uvedylfvy().s[215]++;
    return {
      isValid: errors.length === 0,
      errors,
      warnings
    };
  }
  extractUrls(blocks) {
    /* istanbul ignore next */
    cov_1uvedylfvy().f[29]++;
    const urls =
    /* istanbul ignore next */
    (cov_1uvedylfvy().s[216]++, []);
    /* istanbul ignore next */
    cov_1uvedylfvy().s[217]++;
    blocks.forEach((block, blockIndex) => {
      /* istanbul ignore next */
      cov_1uvedylfvy().f[30]++;
      cov_1uvedylfvy().s[218]++;
      // Extract URLs from different block types
      switch (block.type) {
        case 'bookmark':
          /* istanbul ignore next */
          cov_1uvedylfvy().b[85][0]++;
          cov_1uvedylfvy().s[219]++;
          if (block.bookmark?.url) {
            /* istanbul ignore next */
            cov_1uvedylfvy().b[86][0]++;
            cov_1uvedylfvy().s[220]++;
            urls.push({
              url: block.bookmark.url,
              blockIndex,
              field: 'bookmark.url'
            });
          } else
          /* istanbul ignore next */
          {
            cov_1uvedylfvy().b[86][1]++;
          }
          cov_1uvedylfvy().s[221]++;
          break;
        case 'image':
          /* istanbul ignore next */
          cov_1uvedylfvy().b[85][1]++;
        case 'video':
          /* istanbul ignore next */
          cov_1uvedylfvy().b[85][2]++;
        case 'pdf':
          /* istanbul ignore next */
          cov_1uvedylfvy().b[85][3]++;
          const mediaField =
          /* istanbul ignore next */
          (cov_1uvedylfvy().s[222]++, block[block.type]);
          /* istanbul ignore next */
          cov_1uvedylfvy().s[223]++;
          if (
          /* istanbul ignore next */
          (cov_1uvedylfvy().b[88][0]++, mediaField?.type === 'external') &&
          /* istanbul ignore next */
          (cov_1uvedylfvy().b[88][1]++, mediaField.external?.url)) {
            /* istanbul ignore next */
            cov_1uvedylfvy().b[87][0]++;
            cov_1uvedylfvy().s[224]++;
            urls.push({
              url: mediaField.external.url,
              blockIndex,
              field: `${block.type}.external.url`
            });
          } else
          /* istanbul ignore next */
          {
            cov_1uvedylfvy().b[87][1]++;
          }
          cov_1uvedylfvy().s[225]++;
          break;
        default:
          /* istanbul ignore next */
          cov_1uvedylfvy().b[85][4]++;
          // Extract URLs from rich text links
          const richTextFields =
          /* istanbul ignore next */
          (cov_1uvedylfvy().s[226]++, this.getRichTextFields(block));
          /* istanbul ignore next */
          cov_1uvedylfvy().s[227]++;
          richTextFields.forEach(field => {
            /* istanbul ignore next */
            cov_1uvedylfvy().f[31]++;
            const richText =
            /* istanbul ignore next */
            (cov_1uvedylfvy().s[228]++, this.getNestedProperty(block, field));
            /* istanbul ignore next */
            cov_1uvedylfvy().s[229]++;
            if (Array.isArray(richText)) {
              /* istanbul ignore next */
              cov_1uvedylfvy().b[89][0]++;
              cov_1uvedylfvy().s[230]++;
              richText.forEach((item, itemIndex) => {
                /* istanbul ignore next */
                cov_1uvedylfvy().f[32]++;
                cov_1uvedylfvy().s[231]++;
                if (
                /* istanbul ignore next */
                (cov_1uvedylfvy().b[91][0]++, item.type === 'text') &&
                /* istanbul ignore next */
                (cov_1uvedylfvy().b[91][1]++, item.text?.link?.url)) {
                  /* istanbul ignore next */
                  cov_1uvedylfvy().b[90][0]++;
                  cov_1uvedylfvy().s[232]++;
                  urls.push({
                    url: item.text.link.url,
                    blockIndex,
                    field: `${field}[${itemIndex}].text.link.url`
                  });
                } else
                /* istanbul ignore next */
                {
                  cov_1uvedylfvy().b[90][1]++;
                }
              });
            } else
            /* istanbul ignore next */
            {
              cov_1uvedylfvy().b[89][1]++;
            }
          });
      }
    });
    /* istanbul ignore next */
    cov_1uvedylfvy().s[233]++;
    return urls;
  }
  getRichTextFields(block) {
    /* istanbul ignore next */
    cov_1uvedylfvy().f[33]++;
    cov_1uvedylfvy().s[234]++;
    switch (block.type) {
      case 'paragraph':
        /* istanbul ignore next */
        cov_1uvedylfvy().b[92][0]++;
        cov_1uvedylfvy().s[235]++;
