    // Validate URL
    cov_1uvedylfvy().s[147]++;
    if (
    /* istanbul ignore next */
    (cov_1uvedylfvy().b[53][0]++, mediaField.type === 'external') &&
    /* istanbul ignore next */
    (cov_1uvedylfvy().b[53][1]++, mediaField.external?.url)) {
      /* istanbul ignore next */
      cov_1uvedylfvy().b[52][0]++;
      const url =
      /* istanbul ignore next */
      (cov_1uvedylfvy().s[148]++, mediaField.external.url);
      /* istanbul ignore next */
      cov_1uvedylfvy().s[149]++;
      if (url.length > this.limits.maxUrlLength) {
        /* istanbul ignore next */
        cov_1uvedylfvy().b[54][0]++;
        cov_1uvedylfvy().s[150]++;
        issues.push({
          type: 'warning',
          code: 'URL_TOO_LONG',
          message: `URL length (${url.length}) exceeds recommended limit (${this.limits.maxUrlLength})`
        });
      } else
      /* istanbul ignore next */
      {
        cov_1uvedylfvy().b[54][1]++;
      }
      const urlPattern =
      /* istanbul ignore next */
      (cov_1uvedylfvy().s[151]++, /^https?:\/\/[^\s<>"{}|\\^`[\]]+$/);
      /* istanbul ignore next */
      cov_1uvedylfvy().s[152]++;
      if (
      /* istanbul ignore next */
      (cov_1uvedylfvy().b[56][0]++, !urlPattern.test(url)) ||
      /* istanbul ignore next */
      (cov_1uvedylfvy().b[56][1]++, !url.includes('.'))) {
        /* istanbul ignore next */
        cov_1uvedylfvy().b[55][0]++;
        cov_1uvedylfvy().s[153]++;
        issues.push({
          type: 'error',
          code: 'INVALID_URL',
          message: `Invalid URL: ${url}`
        });
      } else
      /* istanbul ignore next */
      {
        cov_1uvedylfvy().b[55][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_1uvedylfvy().b[52][1]++;
    }
    cov_1uvedylfvy().s[154]++;
    return issues;
  }
