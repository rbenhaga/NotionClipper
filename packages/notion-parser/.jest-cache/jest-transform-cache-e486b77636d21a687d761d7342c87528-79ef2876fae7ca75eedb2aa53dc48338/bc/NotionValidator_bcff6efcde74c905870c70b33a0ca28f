  validateCodeBlock(block, options) {
    /* istanbul ignore next */
    cov_1uvedylfvy().f[14]++;
    const issues =
    /* istanbul ignore next */
    (cov_1uvedylfvy().s[104]++, []);
    /* istanbul ignore next */
    cov_1uvedylfvy().s[105]++;
    if (!block.code) {
      /* istanbul ignore next */
      cov_1uvedylfvy().b[33][0]++;
      cov_1uvedylfvy().s[106]++;
      issues.push({
        type: 'error',
        code: 'MISSING_CODE_CONTENT',
        message: 'Code block is missing code field'
      });
      /* istanbul ignore next */
      cov_1uvedylfvy().s[107]++;
      return issues;
    } else
    /* istanbul ignore next */
    {
      cov_1uvedylfvy().b[33][1]++;
    }
    cov_1uvedylfvy().s[108]++;
    if (
    /* istanbul ignore next */
    (cov_1uvedylfvy().b[35][0]++, options.validateRichText) &&
    /* istanbul ignore next */
    (cov_1uvedylfvy().b[35][1]++, block.code.rich_text)) {
      /* istanbul ignore next */
      cov_1uvedylfvy().b[34][0]++;
      const richTextIssues =
      /* istanbul ignore next */
      (cov_1uvedylfvy().s[109]++, this.validateRichText(block.code.rich_text, 'code.rich_text'));
      // Check code length
      const totalLength =
      /* istanbul ignore next */
      (cov_1uvedylfvy().s[110]++, block.code.rich_text.reduce((sum, item) => {
        /* istanbul ignore next */
        cov_1uvedylfvy().f[15]++;
        cov_1uvedylfvy().s[111]++;
        return sum + (
        /* istanbul ignore next */
        (cov_1uvedylfvy().b[36][0]++, item.text?.content?.length) ||
        /* istanbul ignore next */
        (cov_1uvedylfvy().b[36][1]++, 0));
      }, 0));
      /* istanbul ignore next */
      cov_1uvedylfvy().s[112]++;
      if (totalLength > this.limits.maxCodeLength) {
        /* istanbul ignore next */
        cov_1uvedylfvy().b[37][0]++;
        cov_1uvedylfvy().s[113]++;
        issues.push({
          type: 'warning',
          code: 'CODE_TOO_LONG',
          message: `Code content length (${totalLength}) exceeds recommended limit (${this.limits.maxCodeLength})`
        });
      } else
      /* istanbul ignore next */
      {
        cov_1uvedylfvy().b[37][1]++;
      }
      cov_1uvedylfvy().s[114]++;
      issues.push(...richTextIssues);
    } else
    /* istanbul ignore next */
    {
      cov_1uvedylfvy().b[34][1]++;
    }
    cov_1uvedylfvy().s[115]++;
    return issues;
  }
