  validateMediaBlock(block, _options) {
    /* istanbul ignore next */
    cov_1uvedylfvy().f[19]++;
    const issues =
    /* istanbul ignore next */
    (cov_1uvedylfvy().s[142]++, []);
    const mediaField =
    /* istanbul ignore next */
    (cov_1uvedylfvy().s[143]++, block[block.type]);
    /* istanbul ignore next */
    cov_1uvedylfvy().s[144]++;
