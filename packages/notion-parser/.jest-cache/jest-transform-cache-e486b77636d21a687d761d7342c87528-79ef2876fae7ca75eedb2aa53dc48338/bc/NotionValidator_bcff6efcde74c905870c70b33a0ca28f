  validateEquationBlock(block, _options) {
    /* istanbul ignore next */
    cov_1uvedylfvy().f[21]++;
    const issues =
    /* istanbul ignore next */
    (cov_1uvedylfvy().s[165]++, []);
    /* istanbul ignore next */
    cov_1uvedylfvy().s[166]++;
