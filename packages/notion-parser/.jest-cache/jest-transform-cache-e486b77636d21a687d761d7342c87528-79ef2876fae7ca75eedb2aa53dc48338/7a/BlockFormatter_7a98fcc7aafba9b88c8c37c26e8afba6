        /* istanbul ignore next */
        cov_pg7qapl77().s[212]++;
        result.push(...listItems);
        /* istanbul ignore next */
        cov_pg7qapl77().s[213]++;
        i = j;
      } else {
        /* istanbul ignore next */
        cov_pg7qapl77().b[53][1]++;
        cov_pg7qapl77().s[214]++;
        result.push(block);
        /* istanbul ignore next */
        cov_pg7qapl77().s[215]++;
        i++;
      }
    }
    /* istanbul ignore next */
    cov_pg7qapl77().s[216]++;
    return result;
  }
