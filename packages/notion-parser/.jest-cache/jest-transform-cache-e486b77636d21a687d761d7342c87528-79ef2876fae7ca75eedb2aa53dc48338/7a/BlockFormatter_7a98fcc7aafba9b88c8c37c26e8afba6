      cov_pg7qapl77().s[201]++;
      return clonedBlock;
    });
  }
  optimizeListStructures(blocks) {
    /* istanbul ignore next */
    cov_pg7qapl77().f[33]++;
    // Group consecutive list items
    const result =
    /* istanbul ignore next */
    (cov_pg7qapl77().s[202]++, []);
    let i =
    /* istanbul ignore next */
    (cov_pg7qapl77().s[203]++, 0);
    /* istanbul ignore next */
    cov_pg7qapl77().s[204]++;
    while (i < blocks.length) {
      const block =
      /* istanbul ignore next */
      (cov_pg7qapl77().s[205]++, blocks[i]);
      /* istanbul ignore next */
      cov_pg7qapl77().s[206]++;
