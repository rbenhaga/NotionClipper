    });
  }
  mergeConsecutiveParagraphs(blocks) {
    /* istanbul ignore next */
    cov_pg7qapl77().f[11]++;
    const result =
    /* istanbul ignore next */
    (cov_pg7qapl77().s[84]++, []);
    let currentParagraph =
    /* istanbul ignore next */
    (cov_pg7qapl77().s[85]++, null);
    /* istanbul ignore next */
    cov_pg7qapl77().s[86]++;
    for (const block of blocks) {
      /* istanbul ignore next */
      cov_pg7qapl77().s[87]++;
      if (block.type === 'paragraph') {
        /* istanbul ignore next */
        cov_pg7qapl77().b[22][0]++;
        cov_pg7qapl77().s[88]++;
        if (currentParagraph) {
          /* istanbul ignore next */
          cov_pg7qapl77().b[23][0]++;
          cov_pg7qapl77().s[89]++;
          // Merge with previous paragraph
          currentParagraph.paragraph.rich_text = [...currentParagraph.paragraph.rich_text, {
            type: 'text',
            text: {
              content: ' '
            }
          },
          // Add space between merged paragraphs
          ...block.paragraph.rich_text];
        } else {
          /* istanbul ignore next */
          cov_pg7qapl77().b[23][1]++;
          cov_pg7qapl77().s[90]++;
          currentParagraph = {
            ...block
          };
        }
      } else {
        /* istanbul ignore next */
        cov_pg7qapl77().b[22][1]++;
        cov_pg7qapl77().s[91]++;
        // Push accumulated paragraph if exists
        if (currentParagraph) {
          /* istanbul ignore next */
          cov_pg7qapl77().b[24][0]++;
          cov_pg7qapl77().s[92]++;
          result.push(currentParagraph);
          /* istanbul ignore next */
          cov_pg7qapl77().s[93]++;
          currentParagraph = null;
        } else
        /* istanbul ignore next */
        {
          cov_pg7qapl77().b[24][1]++;
        }
        cov_pg7qapl77().s[94]++;
        result.push(block);
      }
    }
    // Push final paragraph if exists
    /* istanbul ignore next */
    cov_pg7qapl77().s[95]++;
    if (currentParagraph) {
      /* istanbul ignore next */
      cov_pg7qapl77().b[25][0]++;
      cov_pg7qapl77().s[96]++;
      result.push(currentParagraph);
    } else
    /* istanbul ignore next */
    {
      cov_pg7qapl77().b[25][1]++;
    }
    cov_pg7qapl77().s[97]++;
    return result;
  }
  limitConsecutiveEmptyLines(blocks, maxEmpty) {
    /* istanbul ignore next */
    cov_pg7qapl77().f[12]++;
    const result =
    /* istanbul ignore next */
    (cov_pg7qapl77().s[98]++, []);
    let consecutiveEmptyCount =
    /* istanbul ignore next */
    (cov_pg7qapl77().s[99]++, 0);
    /* istanbul ignore next */
    cov_pg7qapl77().s[100]++;
    for (const block of blocks) {
      const isEmpty =
      /* istanbul ignore next */
      (cov_pg7qapl77().s[101]++, this.isEmptyBlock(block));
      /* istanbul ignore next */
      cov_pg7qapl77().s[102]++;
      if (isEmpty) {
        /* istanbul ignore next */
        cov_pg7qapl77().b[26][0]++;
        cov_pg7qapl77().s[103]++;
        consecutiveEmptyCount++;
        /* istanbul ignore next */
        cov_pg7qapl77().s[104]++;
        if (consecutiveEmptyCount <= maxEmpty) {
          /* istanbul ignore next */
          cov_pg7qapl77().b[27][0]++;
          cov_pg7qapl77().s[105]++;
          result.push(block);
        } else
        /* istanbul ignore next */
        {
          cov_pg7qapl77().b[27][1]++;
        }
      } else {
        /* istanbul ignore next */
        cov_pg7qapl77().b[26][1]++;
        cov_pg7qapl77().s[106]++;
        consecutiveEmptyCount = 0;
        /* istanbul ignore next */
        cov_pg7qapl77().s[107]++;
        result.push(block);
      }
    }
    /* istanbul ignore next */
    cov_pg7qapl77().s[108]++;
    return result;
  }
  isEmptyBlock(block) {
    /* istanbul ignore next */
    cov_pg7qapl77().f[13]++;
    cov_pg7qapl77().s[109]++;
    switch (block.type) {
      case 'paragraph':
        /* istanbul ignore next */
        cov_pg7qapl77().b[28][0]++;
        cov_pg7qapl77().s[110]++;
        return !this.hasContent(block.paragraph?.rich_text);
      case 'heading_1':
        /* istanbul ignore next */
        cov_pg7qapl77().b[28][1]++;
      case 'heading_2':
        /* istanbul ignore next */
        cov_pg7qapl77().b[28][2]++;
      case 'heading_3':
        /* istanbul ignore next */
        cov_pg7qapl77().b[28][3]++;
        const headingField =
        /* istanbul ignore next */
        (cov_pg7qapl77().s[111]++, block[block.type]);
        /* istanbul ignore next */
        cov_pg7qapl77().s[112]++;
        return !this.hasContent(headingField?.rich_text);
      default:
        /* istanbul ignore next */
        cov_pg7qapl77().b[28][4]++;
        cov_pg7qapl77().s[113]++;
        return false;
    }
  }
  /**
   * Optimise les blocs pour rduire la taille
   */
  optimize(blocks) {
    /* istanbul ignore next */
    cov_pg7qapl77().f[14]++;
    cov_pg7qapl77().s[114]++;
    return this.format(blocks, {
      removeEmptyBlocks: true,
      normalizeWhitespace: true,
      maxConsecutiveEmptyLines: 1
    });
  }
  /**
   * Applique un style cohrent  tous les blocs
   */
  applyTheme(blocks, theme) {
    /* istanbul ignore next */
    cov_pg7qapl77().f[15]++;
    cov_pg7qapl77().s[115]++;
    return this.format(blocks, {
      color: theme.color,
      applyColorToAll: !!theme.color,
      normalizeWhitespace: true
    });
  }
  mergeSimilarBlocks(blocks) {
    /* istanbul ignore next */
    cov_pg7qapl77().f[16]++;
    const result =
    /* istanbul ignore next */
    (cov_pg7qapl77().s[116]++, []);
    let i =
    /* istanbul ignore next */
    (cov_pg7qapl77().s[117]++, 0);
    /* istanbul ignore next */
    cov_pg7qapl77().s[118]++;
    while (i < blocks.length) {
      const currentBlock =
      /* istanbul ignore next */
      (cov_pg7qapl77().s[119]++, blocks[i]);
      // Look for consecutive blocks of the same type that can be merged
      /* istanbul ignore next */
      cov_pg7qapl77().s[120]++;
      if (this.canMergeBlockType(currentBlock.type)) {
        /* istanbul ignore next */
        cov_pg7qapl77().b[29][0]++;
        const similarBlocks =
        /* istanbul ignore next */
        (cov_pg7qapl77().s[121]++, [currentBlock]);
        let j =
        /* istanbul ignore next */
        (cov_pg7qapl77().s[122]++, i + 1);
        /* istanbul ignore next */
        cov_pg7qapl77().s[123]++;
        while (
        /* istanbul ignore next */
        (cov_pg7qapl77().b[30][0]++, j < blocks.length) &&
        /* istanbul ignore next */
        (cov_pg7qapl77().b[30][1]++, this.areBlocksSimilar(currentBlock, blocks[j]))) {
          /* istanbul ignore next */
          cov_pg7qapl77().s[124]++;
          similarBlocks.push(blocks[j]);
          /* istanbul ignore next */
          cov_pg7qapl77().s[125]++;
          j++;
        }
        /* istanbul ignore next */
        cov_pg7qapl77().s[126]++;
        if (similarBlocks.length > 1) {
          /* istanbul ignore next */
          cov_pg7qapl77().b[31][0]++;
          const mergedBlock =
          /* istanbul ignore next */
          (cov_pg7qapl77().s[127]++, this.mergeBlocks(similarBlocks));
          /* istanbul ignore next */
          cov_pg7qapl77().s[128]++;
          result.push(mergedBlock);
        } else {
          /* istanbul ignore next */
          cov_pg7qapl77().b[31][1]++;
          cov_pg7qapl77().s[129]++;
          result.push(currentBlock);
        }
        /* istanbul ignore next */
        cov_pg7qapl77().s[130]++;
        i = j;
      } else {
        /* istanbul ignore next */
        cov_pg7qapl77().b[29][1]++;
        cov_pg7qapl77().s[131]++;
        result.push(currentBlock);
        /* istanbul ignore next */
        cov_pg7qapl77().s[132]++;
        i++;
      }
    }
    /* istanbul ignore next */
    cov_pg7qapl77().s[133]++;
    return result;
  }
  canMergeBlockType(type) {
    /* istanbul ignore next */
    cov_pg7qapl77().f[17]++;
    cov_pg7qapl77().s[134]++;
    return ['paragraph', 'code'].includes(type);
  }
  areBlocksSimilar(block1, block2) {
    /* istanbul ignore next */
    cov_pg7qapl77().f[18]++;
    cov_pg7qapl77().s[135]++;
    if (block1.type !== block2.type) {
      /* istanbul ignore next */
