        /* istanbul ignore next */
        (cov_pg7qapl77().b[54][0]++, j < blocks.length) &&
        /* istanbul ignore next */
        (cov_pg7qapl77().b[54][1]++, this.isListItem(blocks[j])) &&
        /* istanbul ignore next */
        (cov_pg7qapl77().b[54][2]++, this.isSameListType(block, blocks[j]))) {
          /* istanbul ignore next */
          cov_pg7qapl77().s[210]++;
