    }
    // Merge similar blocks if requested
    cov_pg7qapl77().s[13]++;
    if (options.mergeSimilarBlocks) {
      /* istanbul ignore next */
      cov_pg7qapl77().b[7][0]++;
      cov_pg7qapl77().s[14]++;
      formattedBlocks = this.mergeSimilarBlocks(formattedBlocks);
    } else
    /* istanbul ignore next */
    {
      cov_pg7qapl77().b[7][1]++;
    }
    // Trim rich text if requested
    cov_pg7qapl77().s[15]++;
    if (options.trimRichText) {
      /* istanbul ignore next */
      cov_pg7qapl77().b[8][0]++;
      cov_pg7qapl77().s[16]++;
      formattedBlocks = this.trimRichText(formattedBlocks);
    } else
    /* istanbul ignore next */
    {
      cov_pg7qapl77().b[8][1]++;
    }
    // Enforce block limits if requested
    cov_pg7qapl77().s[17]++;
    if (options.enforceBlockLimits) {
      /* istanbul ignore next */
      cov_pg7qapl77().b[9][0]++;
      cov_pg7qapl77().s[18]++;
      formattedBlocks = this.enforceBlockLimits(formattedBlocks, options);
    } else
    /* istanbul ignore next */
    {
      cov_pg7qapl77().b[9][1]++;
    }
    // Optimize structure if requested
    cov_pg7qapl77().s[19]++;
    if (options.optimizeStructure) {
      /* istanbul ignore next */
      cov_pg7qapl77().b[10][0]++;
      cov_pg7qapl77().s[20]++;
      formattedBlocks = this.optimizeStructure(formattedBlocks, options);
    } else
    /* istanbul ignore next */
    {
      cov_pg7qapl77().b[10][1]++;
    }
    cov_pg7qapl77().s[21]++;
    return formattedBlocks;
  }
  removeEmptyBlocks(blocks) {
    /* istanbul ignore next */
    cov_pg7qapl77().f[1]++;
    cov_pg7qapl77().s[22]++;
    return blocks.filter(block => {
      /* istanbul ignore next */
      cov_pg7qapl77().f[2]++;
      cov_pg7qapl77().s[23]++;
      switch (block.type) {
        case 'paragraph':
          /* istanbul ignore next */
          cov_pg7qapl77().b[11][0]++;
          cov_pg7qapl77().s[24]++;
          return this.hasContent(block.paragraph?.rich_text);
        case 'heading_1':
          /* istanbul ignore next */
          cov_pg7qapl77().b[11][1]++;
        case 'heading_2':
          /* istanbul ignore next */
          cov_pg7qapl77().b[11][2]++;
        case 'heading_3':
          /* istanbul ignore next */
          cov_pg7qapl77().b[11][3]++;
          const headingField =
          /* istanbul ignore next */
          (cov_pg7qapl77().s[25]++, block[block.type]);
          /* istanbul ignore next */
          cov_pg7qapl77().s[26]++;
          return this.hasContent(headingField?.rich_text);
        case 'bulleted_list_item':
          /* istanbul ignore next */
          cov_pg7qapl77().b[11][4]++;
        case 'numbered_list_item':
          /* istanbul ignore next */
          cov_pg7qapl77().b[11][5]++;
          const listField =
          /* istanbul ignore next */
          (cov_pg7qapl77().s[27]++, block[block.type]);
          /* istanbul ignore next */
          cov_pg7qapl77().s[28]++;
          return this.hasContent(listField?.rich_text);
        case 'to_do':
          /* istanbul ignore next */
          cov_pg7qapl77().b[11][6]++;
          cov_pg7qapl77().s[29]++;
          return this.hasContent(block.to_do?.rich_text);
        case 'toggle':
          /* istanbul ignore next */
          cov_pg7qapl77().b[11][7]++;
          cov_pg7qapl77().s[30]++;
          return this.hasContent(block.toggle?.rich_text);
        case 'quote':
          /* istanbul ignore next */
          cov_pg7qapl77().b[11][8]++;
          cov_pg7qapl77().s[31]++;
          return this.hasContent(block.quote?.rich_text);
        case 'callout':
          /* istanbul ignore next */
          cov_pg7qapl77().b[11][9]++;
          cov_pg7qapl77().s[32]++;
          return this.hasContent(block.callout?.rich_text);
        case 'code':
          /* istanbul ignore next */
          cov_pg7qapl77().b[11][10]++;
          cov_pg7qapl77().s[33]++;
          return this.hasContent(block.code?.rich_text);
        default:
          /* istanbul ignore next */
          cov_pg7qapl77().b[11][11]++;
          cov_pg7qapl77().s[34]++;
          return true;
        // Keep other block types
      }
    });
  }
  hasContent(richText) {
    /* istanbul ignore next */
    cov_pg7qapl77().f[3]++;
    cov_pg7qapl77().s[35]++;
    if (!Array.isArray(richText)) {
      /* istanbul ignore next */
      cov_pg7qapl77().b[12][0]++;
      cov_pg7qapl77().s[36]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_pg7qapl77().b[12][1]++;
    }
    cov_pg7qapl77().s[37]++;
    return richText.some(item => {
      /* istanbul ignore next */
      cov_pg7qapl77().f[4]++;
      const content =
      /* istanbul ignore next */
      (cov_pg7qapl77().s[38]++, item.type === 'text' ?
      /* istanbul ignore next */
      (cov_pg7qapl77().b[13][0]++,
      /* istanbul ignore next */
      (cov_pg7qapl77().b[14][0]++, item.text?.content) ||
      /* istanbul ignore next */
      (cov_pg7qapl77().b[14][1]++, '')) :
      /* istanbul ignore next */
      (cov_pg7qapl77().b[13][1]++, item.type === 'equation' ?
      /* istanbul ignore next */
      (cov_pg7qapl77().b[15][0]++,
      /* istanbul ignore next */
      (cov_pg7qapl77().b[16][0]++, item.equation?.expression) ||
      /* istanbul ignore next */
      (cov_pg7qapl77().b[16][1]++, '')) :
      /* istanbul ignore next */
      (cov_pg7qapl77().b[15][1]++, '')));
      /* istanbul ignore next */
      cov_pg7qapl77().s[39]++;
      return content.trim().length > 0;
    });
  }
  normalizeWhitespace(blocks) {
    /* istanbul ignore next */
    cov_pg7qapl77().f[5]++;
    cov_pg7qapl77().s[40]++;
    return blocks.map(block => {
      /* istanbul ignore next */
      cov_pg7qapl77().f[6]++;
      const clonedBlock =
      /* istanbul ignore next */
      (cov_pg7qapl77().s[41]++, {
        ...block
      });
      /* istanbul ignore next */
      cov_pg7qapl77().s[42]++;
      switch (block.type) {
        case 'paragraph':
          /* istanbul ignore next */
          cov_pg7qapl77().b[17][0]++;
          cov_pg7qapl77().s[43]++;
          clonedBlock.paragraph = {
            ...clonedBlock.paragraph,
            rich_text: this.normalizeRichTextWhitespace(clonedBlock.paragraph?.rich_text)
          };
          /* istanbul ignore next */
          cov_pg7qapl77().s[44]++;
          break;
        case 'heading_1':
          /* istanbul ignore next */
          cov_pg7qapl77().b[17][1]++;
        case 'heading_2':
          /* istanbul ignore next */
          cov_pg7qapl77().b[17][2]++;
        case 'heading_3':
          /* istanbul ignore next */
          cov_pg7qapl77().b[17][3]++;
          const headingField =
          /* istanbul ignore next */
          (cov_pg7qapl77().s[45]++, clonedBlock[block.type]);
          /* istanbul ignore next */
          cov_pg7qapl77().s[46]++;
          clonedBlock[block.type] = {
            ...headingField,
            rich_text: this.normalizeRichTextWhitespace(headingField?.rich_text)
          };
          /* istanbul ignore next */
          cov_pg7qapl77().s[47]++;
          break;
        case 'bulleted_list_item':
          /* istanbul ignore next */
          cov_pg7qapl77().b[17][4]++;
        case 'numbered_list_item':
          /* istanbul ignore next */
          cov_pg7qapl77().b[17][5]++;
          const listField =
          /* istanbul ignore next */
          (cov_pg7qapl77().s[48]++, clonedBlock[block.type]);
          /* istanbul ignore next */
          cov_pg7qapl77().s[49]++;
          clonedBlock[block.type] = {
            ...listField,
            rich_text: this.normalizeRichTextWhitespace(listField?.rich_text)
          };
          /* istanbul ignore next */
          cov_pg7qapl77().s[50]++;
          break;
        case 'to_do':
          /* istanbul ignore next */
          cov_pg7qapl77().b[17][6]++;
          cov_pg7qapl77().s[51]++;
          clonedBlock.to_do = {
            ...clonedBlock.to_do,
            rich_text: this.normalizeRichTextWhitespace(clonedBlock.to_do?.rich_text)
          };
          /* istanbul ignore next */
          cov_pg7qapl77().s[52]++;
          break;
        case 'toggle':
          /* istanbul ignore next */
          cov_pg7qapl77().b[17][7]++;
          cov_pg7qapl77().s[53]++;
          clonedBlock.toggle = {
            ...clonedBlock.toggle,
            rich_text: this.normalizeRichTextWhitespace(clonedBlock.toggle?.rich_text)
          };
          /* istanbul ignore next */
          cov_pg7qapl77().s[54]++;
          break;
        case 'quote':
          /* istanbul ignore next */
          cov_pg7qapl77().b[17][8]++;
          cov_pg7qapl77().s[55]++;
          clonedBlock.quote = {
            ...clonedBlock.quote,
            rich_text: this.normalizeRichTextWhitespace(clonedBlock.quote?.rich_text)
          };
          /* istanbul ignore next */
          cov_pg7qapl77().s[56]++;
          break;
        case 'callout':
          /* istanbul ignore next */
          cov_pg7qapl77().b[17][9]++;
          cov_pg7qapl77().s[57]++;
          clonedBlock.callout = {
            ...clonedBlock.callout,
            rich_text: this.normalizeRichTextWhitespace(clonedBlock.callout?.rich_text)
          };
          /* istanbul ignore next */
          cov_pg7qapl77().s[58]++;
          break;
      }
      /* istanbul ignore next */
      cov_pg7qapl77().s[59]++;
      return clonedBlock;
    });
  }
  normalizeRichTextWhitespace(richText) {
    /* istanbul ignore next */
    cov_pg7qapl77().f[7]++;
    cov_pg7qapl77().s[60]++;
    if (!Array.isArray(richText)) {
      /* istanbul ignore next */
      cov_pg7qapl77().b[18][0]++;
      cov_pg7qapl77().s[61]++;
      return richText;
    } else
    /* istanbul ignore next */
    {
      cov_pg7qapl77().b[18][1]++;
    }
    cov_pg7qapl77().s[62]++;
    return richText.map(item => {
      /* istanbul ignore next */
      cov_pg7qapl77().f[8]++;
      cov_pg7qapl77().s[63]++;
      if (
      /* istanbul ignore next */
      (cov_pg7qapl77().b[20][0]++, item.type === 'text') &&
      /* istanbul ignore next */
      (cov_pg7qapl77().b[20][1]++, item.text?.content)) {
        /* istanbul ignore next */
        cov_pg7qapl77().b[19][0]++;
        cov_pg7qapl77().s[64]++;
        return {
          ...item,
          text: {
            ...item.text,
            content: item.text.content.replace(/\r\n/g, '\n').replace(/\r/g, '\n').replace(/\t/g, '  ').replace(/[ ]+/g, ' ').trim()
          }
        };
      } else
      /* istanbul ignore next */
      {
        cov_pg7qapl77().b[19][1]++;
      }
      cov_pg7qapl77().s[65]++;
      return item;
    });
  }
  applyColor(blocks, color) {
    /* istanbul ignore next */
    cov_pg7qapl77().f[9]++;
    cov_pg7qapl77().s[66]++;
    return blocks.map(block => {
      /* istanbul ignore next */
      cov_pg7qapl77().f[10]++;
      const clonedBlock =
      /* istanbul ignore next */
      (cov_pg7qapl77().s[67]++, {
        ...block
      });
      /* istanbul ignore next */
      cov_pg7qapl77().s[68]++;
      switch (block.type) {
        case 'paragraph':
