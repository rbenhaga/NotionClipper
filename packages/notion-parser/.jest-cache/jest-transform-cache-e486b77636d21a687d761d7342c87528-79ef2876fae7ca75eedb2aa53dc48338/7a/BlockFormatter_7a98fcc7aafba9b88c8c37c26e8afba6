  getNestedProperty(obj, path) {
    /* istanbul ignore next */
    cov_pg7qapl77().f[39]++;
    cov_pg7qapl77().s[240]++;
    return path.split('.').reduce((current, key) => {
      /* istanbul ignore next */
      cov_pg7qapl77().f[40]++;
      cov_pg7qapl77().s[241]++;
      return current?.[key];
    }, obj);
  }
  setNestedProperty(obj, path, value) {
    /* istanbul ignore next */
    cov_pg7qapl77().f[41]++;
    const keys =
    /* istanbul ignore next */
    (cov_pg7qapl77().s[242]++, path.split('.'));
    const lastKey =
    /* istanbul ignore next */
    (cov_pg7qapl77().s[243]++, keys.pop());
    const target =
    /* istanbul ignore next */
    (cov_pg7qapl77().s[244]++, keys.reduce((current, key) => {
      /* istanbul ignore next */
      cov_pg7qapl77().f[42]++;
      cov_pg7qapl77().s[245]++;
      if (!current[key]) {
        /* istanbul ignore next */
        cov_pg7qapl77().b[58][0]++;
        cov_pg7qapl77().s[246]++;
        current[key] = {};
      } else
      /* istanbul ignore next */
      {
        cov_pg7qapl77().b[58][1]++;
      }
      cov_pg7qapl77().s[247]++;
      return current[key];
    }, obj));
    /* istanbul ignore next */
    cov_pg7qapl77().s[248]++;
    target[lastKey] = value;
  }
}
