      /* istanbul ignore next */
      (cov_pg7qapl77().b[56][0]++, !lastBlock) ||
      /* istanbul ignore next */
      (cov_pg7qapl77().b[56][1]++, !this.areBlocksIdentical(lastBlock, block))) {
