        cov_2d6smxpl92().s[21]++;
        return ''; // URL bloquée
      } else
      /* istanbul ignore next */
      {
        cov_2d6smxpl92().b[4][1]++;
      }
    }
    // Valider que c'est une URL HTTP/HTTPS valide
    /* istanbul ignore next */
    cov_2d6smxpl92().s[22]++;
    try {
      const urlObj =
      /* istanbul ignore next */
      (cov_2d6smxpl92().s[23]++, new URL(url));
      /* istanbul ignore next */
      cov_2d6smxpl92().s[24]++;
      if (
      /* istanbul ignore next */
      (cov_2d6smxpl92().b[6][0]++, urlObj.protocol !== 'http:') &&
      /* istanbul ignore next */
      (cov_2d6smxpl92().b[6][1]++, urlObj.protocol !== 'https:')) {
        /* istanbul ignore next */
        cov_2d6smxpl92().b[5][0]++;
        cov_2d6smxpl92().s[25]++;
        return '';
      } else
      /* istanbul ignore next */
      {
        cov_2d6smxpl92().b[5][1]++;
      }
      cov_2d6smxpl92().s[26]++;
      return url;
    } catch {
      /* istanbul ignore next */
      cov_2d6smxpl92().s[27]++;
      return ''; // URL invalide
    }
  }
  /**
   * Sanitise le contenu d'une cellule de table (protection CSV injection)
   */
  static sanitizeTableCell(cell) {
    /* istanbul ignore next */
    cov_2d6smxpl92().f[4]++;
    cov_2d6smxpl92().s[28]++;
    if (
    /* istanbul ignore next */
    (cov_2d6smxpl92().b[8][0]++, !cell) ||
    /* istanbul ignore next */
    (cov_2d6smxpl92().b[8][1]++, typeof cell !== 'string')) {
      /* istanbul ignore next */
      cov_2d6smxpl92().b[7][0]++;
      cov_2d6smxpl92().s[29]++;
      return '';
    } else
    /* istanbul ignore next */
    {
      cov_2d6smxpl92().b[7][1]++;
    }
    let sanitized =
    /* istanbul ignore next */
    (cov_2d6smxpl92().s[30]++, this.sanitizeText(cell));
    // Supprimer les préfixes de formule dangereux
    /* istanbul ignore next */
    cov_2d6smxpl92().s[31]++;
    if (this.FORMULA_PREFIXES.some(prefix => {
      /* istanbul ignore next */
      cov_2d6smxpl92().f[5]++;
      cov_2d6smxpl92().s[32]++;
      return sanitized.startsWith(prefix);
    })) {
      /* istanbul ignore next */
      cov_2d6smxpl92().b[9][0]++;
      cov_2d6smxpl92().s[33]++;
      sanitized = sanitized.substring(1); // Supprimer le premier caractère
    } else
    /* istanbul ignore next */
    {
      cov_2d6smxpl92().b[9][1]++;
    }
    // Supprimer les commandes dangereuses
    const dangerousCommands =
    /* istanbul ignore next */
    (cov_2d6smxpl92().s[34]++, ['cmd', 'calc', 'HYPERLINK', 'SUM', 'EXEC']);
    /* istanbul ignore next */
    cov_2d6smxpl92().s[35]++;
    dangerousCommands.forEach(cmd => {
      /* istanbul ignore next */
      cov_2d6smxpl92().f[6]++;
      const regex =
      /* istanbul ignore next */
      (cov_2d6smxpl92().s[36]++, new RegExp(cmd, 'gi'));
      /* istanbul ignore next */
      cov_2d6smxpl92().s[37]++;
      sanitized = sanitized.replace(regex, '');
    });
    /* istanbul ignore next */
    cov_2d6smxpl92().s[38]++;
    return sanitized;
  }
  /**
   * Sanitise le contenu complet (utilisé par les parsers)
   */
  static sanitizeContent(content) {
    /* istanbul ignore next */
    cov_2d6smxpl92().f[7]++;
    cov_2d6smxpl92().s[39]++;
    if (
    /* istanbul ignore next */
    (cov_2d6smxpl92().b[11][0]++, !content) ||
    /* istanbul ignore next */
    (cov_2d6smxpl92().b[11][1]++, typeof content !== 'string')) {
      /* istanbul ignore next */
      cov_2d6smxpl92().b[10][0]++;
      cov_2d6smxpl92().s[40]++;
      return '';
    } else
    /* istanbul ignore next */
    {
      cov_2d6smxpl92().b[10][1]++;
    }
    let sanitized =
    /* istanbul ignore next */
    (cov_2d6smxpl92().s[41]++, content);
    // Sanitisation générale du texte
    /* istanbul ignore next */
    cov_2d6smxpl92().s[42]++;
    sanitized = this.sanitizeText(sanitized);
    // Normalisation Unicode pour éviter les bypasses
    /* istanbul ignore next */
    cov_2d6smxpl92().s[43]++;
    sanitized = sanitized.normalize('NFC');
    /* istanbul ignore next */
    cov_2d6smxpl92().s[44]++;
    return sanitized;
  }
  /**
   * Vérifie si une URL est sûre
   */
  static isUrlSafe(url) {
    /* istanbul ignore next */
    cov_2d6smxpl92().f[8]++;
    cov_2d6smxpl92().s[45]++;
    return this.sanitizeUrl(url) === url;
  }
  /**
   * Vérifie si le contenu contient des éléments dangereux
   */
  static containsDangerousContent(content) {
    /* istanbul ignore next */
    cov_2d6smxpl92().f[9]++;
    cov_2d6smxpl92().s[46]++;
    if (
    /* istanbul ignore next */
    (cov_2d6smxpl92().b[13][0]++, !content) ||
    /* istanbul ignore next */
    (cov_2d6smxpl92().b[13][1]++, typeof content !== 'string')) {
      /* istanbul ignore next */
      cov_2d6smxpl92().b[12][0]++;
      cov_2d6smxpl92().s[47]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_2d6smxpl92().b[12][1]++;
    }
    const lowerContent =
    /* istanbul ignore next */
    (cov_2d6smxpl92().s[48]++, content.toLowerCase());
    // Vérifier les balises dangereuses
    /* istanbul ignore next */
    cov_2d6smxpl92().s[49]++;
    if (this.DANGEROUS_TAGS.some(tag => {
      /* istanbul ignore next */
      cov_2d6smxpl92().f[10]++;
      cov_2d6smxpl92().s[50]++;
      return lowerContent.includes(tag);
    })) {
      /* istanbul ignore next */
      cov_2d6smxpl92().b[14][0]++;
      cov_2d6smxpl92().s[51]++;
      return true;
    } else
    /* istanbul ignore next */
    {
      cov_2d6smxpl92().b[14][1]++;
    }
    // Vérifier les attributs dangereux
    cov_2d6smxpl92().s[52]++;
    if (this.DANGEROUS_ATTRIBUTES.some(attr => {
      /* istanbul ignore next */
      cov_2d6smxpl92().f[11]++;
      cov_2d6smxpl92().s[53]++;
      return lowerContent.includes(attr);
    })) {
      /* istanbul ignore next */
      cov_2d6smxpl92().b[15][0]++;
      cov_2d6smxpl92().s[54]++;
      return true;
    } else
    /* istanbul ignore next */
    {
      cov_2d6smxpl92().b[15][1]++;
    }
    // Vérifier les protocoles dangereux
    cov_2d6smxpl92().s[55]++;
    if (this.DANGEROUS_PROTOCOLS.some(protocol => {
      /* istanbul ignore next */
      cov_2d6smxpl92().f[12]++;
      cov_2d6smxpl92().s[56]++;
      return lowerContent.includes(protocol);
    })) {
      /* istanbul ignore next */
      cov_2d6smxpl92().b[16][0]++;
      cov_2d6smxpl92().s[57]++;
      return true;
    } else
    /* istanbul ignore next */
    {
      cov_2d6smxpl92().b[16][1]++;
    }
    cov_2d6smxpl92().s[58]++;
    return false;
  }
