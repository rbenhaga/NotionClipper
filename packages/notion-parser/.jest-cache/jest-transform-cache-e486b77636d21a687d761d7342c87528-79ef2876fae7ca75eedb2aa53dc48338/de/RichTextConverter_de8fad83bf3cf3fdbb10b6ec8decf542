          /* istanbul ignore next */
          cov_27sem404wr().b[3][0]++;
          cov_27sem404wr().s[39]++;
          content = match[1];
          /* istanbul ignore next */
          cov_27sem404wr().s[40]++;
          url = ContentSanitizer_1.ContentSanitizer.sanitizeUrl(match[2]);
        } else {
          /* istanbul ignore next */
          cov_27sem404wr().b[3][1]++;
          cov_27sem404wr().s[41]++;
          if (pattern.type === 'auto-link') {
            /* istanbul ignore next */
            cov_27sem404wr().b[4][0]++;
            cov_27sem404wr().s[42]++;
            content = match[1];
            /* istanbul ignore next */
            cov_27sem404wr().s[43]++;
            url = ContentSanitizer_1.ContentSanitizer.sanitizeUrl(match[1]);
          } else {
            /* istanbul ignore next */
            cov_27sem404wr().b[4][1]++;
            cov_27sem404wr().s[44]++;
            if (pattern.type === 'equation') {
              /* istanbul ignore next */
              cov_27sem404wr().b[5][0]++;
              cov_27sem404wr().s[45]++;
              expression = match[1];
              /* istanbul ignore next */
              cov_27sem404wr().s[46]++;
              content = match[1];
            } else {
              /* istanbul ignore next */
              cov_27sem404wr().b[5][1]++;
              cov_27sem404wr().s[47]++;
              if (pattern.type === 'bold-italic') {
                /* istanbul ignore next */
                cov_27sem404wr().b[6][0]++;
                cov_27sem404wr().s[48]++;
                // Traiter comme bold ET italic
                allMatches.push({
                  match,
                  type: 'bold',
                  priority: pattern.priority,
                  start,
                  end,
                  content
                });
                /* istanbul ignore next */
                cov_27sem404wr().s[49]++;
                allMatches.push({
                  match,
                  type: 'italic',
                  priority: pattern.priority,
                  start,
                  end,
                  content
                });
                /* istanbul ignore next */
                cov_27sem404wr().s[50]++;
                continue;
              } else
              /* istanbul ignore next */
              {
                cov_27sem404wr().b[6][1]++;
              }
            }
          }
        }
        /* istanbul ignore next */
        cov_27sem404wr().s[51]++;
        allMatches.push({
          match,
          type: pattern.type,
          priority: pattern.priority,
          start,
          end,
          content,
          url,
          expression
        });
      }
    });
    // Trier par position puis par priorit
    /* istanbul ignore next */
    cov_27sem404wr().s[52]++;
    allMatches.sort((a, b) => {
      /* istanbul ignore next */
      cov_27sem404wr().f[8]++;
      cov_27sem404wr().s[53]++;
      if (a.start !== b.start) {
        /* istanbul ignore next */
        cov_27sem404wr().b[7][0]++;
        cov_27sem404wr().s[54]++;
        return a.start - b.start;
      } else
      /* istanbul ignore next */
      {
        cov_27sem404wr().b[7][1]++;
      }
      cov_27sem404wr().s[55]++;
      return a.priority - b.priority;
    });
    /* istanbul ignore next */
    cov_27sem404wr().s[56]++;
    return allMatches.map(m => {
      /* istanbul ignore next */
      cov_27sem404wr().f[9]++;
      cov_27sem404wr().s[57]++;
      return {
        type: m.type,
        content: m.content,
        start: m.start,
        end: m.end,
        url: m.url,
        expression: m.expression
      };
    });
  }
  tokensToRichText(tokens, originalText) {
    /* istanbul ignore next */
    cov_27sem404wr().f[10]++;
    cov_27sem404wr().s[58]++;
    if (tokens.length === 0) {
      /* istanbul ignore next */
      cov_27sem404wr().b[8][0]++;
      cov_27sem404wr().s[59]++;
      return [{
        type: 'text',
        text: {
          content: this.restoreEscapes(originalText)
        }
      }];
    } else
    /* istanbul ignore next */
    {
      cov_27sem404wr().b[8][1]++;
    }
    const result =
    /* istanbul ignore next */
    (cov_27sem404wr().s[60]++, []);
    const segments =
    /* istanbul ignore next */
    (cov_27sem404wr().s[61]++, this.createSegments(tokens, originalText));
    /* istanbul ignore next */
    cov_27sem404wr().s[62]++;
    segments.forEach(segment => {
      /* istanbul ignore next */
      cov_27sem404wr().f[11]++;
      cov_27sem404wr().s[63]++;
      if (segment.type === 'equation') {
        /* istanbul ignore next */
        cov_27sem404wr().b[9][0]++;
        cov_27sem404wr().s[64]++;
        result.push({
          type: 'equation',
          equation: {
            expression:
            /* istanbul ignore next */
            (cov_27sem404wr().b[10][0]++, segment.expression) ||
            /* istanbul ignore next */
            (cov_27sem404wr().b[10][1]++, segment.content)
          }
        });
      } else {
        /* istanbul ignore next */
        cov_27sem404wr().b[9][1]++;
        const annotations =
        /* istanbul ignore next */
        (cov_27sem404wr().s[65]++, {});
        // Appliquer toutes les annotations qui s'appliquent  ce segment
        /* istanbul ignore next */
        cov_27sem404wr().s[66]++;
        if (segment.bold) {
          /* istanbul ignore next */
          cov_27sem404wr().b[11][0]++;
          cov_27sem404wr().s[67]++;
          annotations.bold = true;
        } else
        /* istanbul ignore next */
        {
          cov_27sem404wr().b[11][1]++;
        }
        cov_27sem404wr().s[68]++;
        if (segment.italic) {
          /* istanbul ignore next */
          cov_27sem404wr().b[12][0]++;
          cov_27sem404wr().s[69]++;
          annotations.italic = true;
        } else
        /* istanbul ignore next */
        {
          cov_27sem404wr().b[12][1]++;
        }
        cov_27sem404wr().s[70]++;
        if (segment.code) {
          /* istanbul ignore next */
          cov_27sem404wr().b[13][0]++;
          cov_27sem404wr().s[71]++;
          annotations.code = true;
        } else
        /* istanbul ignore next */
        {
          cov_27sem404wr().b[13][1]++;
        }
        cov_27sem404wr().s[72]++;
        if (segment.strikethrough) {
          /* istanbul ignore next */
          cov_27sem404wr().b[14][0]++;
          cov_27sem404wr().s[73]++;
          annotations.strikethrough = true;
        } else
        /* istanbul ignore next */
        {
          cov_27sem404wr().b[14][1]++;
        }
        cov_27sem404wr().s[74]++;
        if (segment.underline) {
          /* istanbul ignore next */
          cov_27sem404wr().b[15][0]++;
          cov_27sem404wr().s[75]++;
          annotations.underline = true;
        } else
        /* istanbul ignore next */
        {
          cov_27sem404wr().b[15][1]++;
        }
        const textContent =
        /* istanbul ignore next */
        (cov_27sem404wr().s[76]++, {
          content: segment.content
        });
        /* istanbul ignore next */
        cov_27sem404wr().s[77]++;
        if (segment.link) {
          /* istanbul ignore next */
          cov_27sem404wr().b[16][0]++;
          cov_27sem404wr().s[78]++;
          textContent.link = {
            url: segment.link.url
          };
        } else
        /* istanbul ignore next */
        {
          cov_27sem404wr().b[16][1]++;
        }
        // Restaurer les chappements dans le contenu
        cov_27sem404wr().s[79]++;
        textContent.content = this.restoreEscapes(textContent.content);
        /* istanbul ignore next */
        cov_27sem404wr().s[80]++;
        result.push({
          type: 'text',
          text: textContent,
          annotations: Object.keys(annotations).length > 0 ?
          /* istanbul ignore next */
          (cov_27sem404wr().b[17][0]++, annotations) :
          /* istanbul ignore next */
          (cov_27sem404wr().b[17][1]++, undefined),
          href: segment.link ?
          /* istanbul ignore next */
          (cov_27sem404wr().b[18][0]++, segment.link.url) :
          /* istanbul ignore next */
          (cov_27sem404wr().b[18][1]++, null)
        });
      }
    });
    /* istanbul ignore next */
    cov_27sem404wr().s[81]++;
    return result;
  }
  createSegments(tokens, originalText) {
    /* istanbul ignore next */
    cov_27sem404wr().f[12]++;
    // Crer une liste de tous les points de changement
    const changePoints =
    /* istanbul ignore next */
    (cov_27sem404wr().s[82]++, new Set());
    /* istanbul ignore next */
    cov_27sem404wr().s[83]++;
    changePoints.add(0);
    /* istanbul ignore next */
    cov_27sem404wr().s[84]++;
    changePoints.add(originalText.length);
    /* istanbul ignore next */
    cov_27sem404wr().s[85]++;
    tokens.forEach(token => {
      /* istanbul ignore next */
      cov_27sem404wr().f[13]++;
      cov_27sem404wr().s[86]++;
      changePoints.add(token.start);
      /* istanbul ignore next */
      cov_27sem404wr().s[87]++;
      changePoints.add(token.end);
    });
    const sortedPoints =
    /* istanbul ignore next */
    (cov_27sem404wr().s[88]++, Array.from(changePoints).sort((a, b) => {
      /* istanbul ignore next */
      cov_27sem404wr().f[14]++;
      cov_27sem404wr().s[89]++;
      return a - b;
    }));
    const segments =
    /* istanbul ignore next */
    (cov_27sem404wr().s[90]++, []);
    // Crer des segments entre chaque point de changement
    /* istanbul ignore next */
    cov_27sem404wr().s[91]++;
    for (let i =
    /* istanbul ignore next */
    (cov_27sem404wr().s[92]++, 0); i < sortedPoints.length - 1; i++) {
      const start =
      /* istanbul ignore next */
      (cov_27sem404wr().s[93]++, sortedPoints[i]);
      const end =
      /* istanbul ignore next */
      (cov_27sem404wr().s[94]++, sortedPoints[i + 1]);
      /* istanbul ignore next */
      cov_27sem404wr().s[95]++;
      if (start === end) {
        /* istanbul ignore next */
        cov_27sem404wr().b[19][0]++;
        cov_27sem404wr().s[96]++;
        continue;
      } else
      /* istanbul ignore next */
      {
        cov_27sem404wr().b[19][1]++;
      }
      const content =
      /* istanbul ignore next */
      (cov_27sem404wr().s[97]++, originalText.slice(start, end));
      /* istanbul ignore next */
      cov_27sem404wr().s[98]++;
      if (!content) {
        /* istanbul ignore next */
        cov_27sem404wr().b[20][0]++;
        cov_27sem404wr().s[99]++;
        continue;
      } else
      /* istanbul ignore next */
      {
        cov_27sem404wr().b[20][1]++;
      }
      // Dterminer quels tokens s'appliquent  ce segment
      const applicableTokens =
      /* istanbul ignore next */
      (cov_27sem404wr().s[100]++, tokens.filter(token => {
        /* istanbul ignore next */
        cov_27sem404wr().f[15]++;
        cov_27sem404wr().s[101]++;
        // Un token s'applique si le segment est compltement  l'intrieur du token
        return /* istanbul ignore next */(cov_27sem404wr().b[21][0]++, token.start <= start) &&
        /* istanbul ignore next */
        (cov_27sem404wr().b[21][1]++, end <= token.end);
      }));
      // Vrifier si c'est une quation (priorit absolue)
      const equationToken =
      /* istanbul ignore next */
      (cov_27sem404wr().s[102]++, applicableTokens.find(t => {
        /* istanbul ignore next */
        cov_27sem404wr().f[16]++;
        cov_27sem404wr().s[103]++;
        return t.type === 'equation';
      }));
      /* istanbul ignore next */
      cov_27sem404wr().s[104]++;
      if (equationToken) {
        /* istanbul ignore next */
        cov_27sem404wr().b[22][0]++;
        cov_27sem404wr().s[105]++;
        segments.push({
          content: equationToken.content,
          start,
          end,
          type: 'equation',
          expression: equationToken.expression
        });
        /* istanbul ignore next */
        cov_27sem404wr().s[106]++;
        continue;
      } else
      /* istanbul ignore next */
      {
        cov_27sem404wr().b[22][1]++;
      }
      // Construire les annotations pour ce segment
      const segment =
      /* istanbul ignore next */
      (cov_27sem404wr().s[107]++, {
        content,
        start,
        end,
        type: 'text'
      });
      /* istanbul ignore next */
      cov_27sem404wr().s[108]++;
      applicableTokens.forEach(token => {
        /* istanbul ignore next */
        cov_27sem404wr().f[17]++;
        cov_27sem404wr().s[109]++;
        switch (token.type) {
          case 'bold':
            /* istanbul ignore next */
            cov_27sem404wr().b[23][0]++;
            cov_27sem404wr().s[110]++;
            segment.bold = true;
            /* istanbul ignore next */
            cov_27sem404wr().s[111]++;
            break;
          case 'italic':
            /* istanbul ignore next */
            cov_27sem404wr().b[23][1]++;
            cov_27sem404wr().s[112]++;
            segment.italic = true;
            /* istanbul ignore next */
            cov_27sem404wr().s[113]++;
            break;
          case 'code':
            /* istanbul ignore next */
            cov_27sem404wr().b[23][2]++;
            cov_27sem404wr().s[114]++;
            segment.code = true;
            /* istanbul ignore next */
            cov_27sem404wr().s[115]++;
            break;
          case 'strikethrough':
            /* istanbul ignore next */
            cov_27sem404wr().b[23][3]++;
            cov_27sem404wr().s[116]++;
            segment.strikethrough = true;
            /* istanbul ignore next */
            cov_27sem404wr().s[117]++;
            break;
          case 'underline':
            /* istanbul ignore next */
            cov_27sem404wr().b[23][4]++;
            cov_27sem404wr().s[118]++;
            segment.underline = true;
            /* istanbul ignore next */
            cov_27sem404wr().s[119]++;
            break;
          case 'link':
            /* istanbul ignore next */
            cov_27sem404wr().b[23][5]++;
          case 'auto-link':
            /* istanbul ignore next */
            cov_27sem404wr().b[23][6]++;
            cov_27sem404wr().s[120]++;
            segment.link = {
              url: token.url
            };
            /* istanbul ignore next */
            cov_27sem404wr().s[121]++;
            break;
        }
      });
      /* istanbul ignore next */
      cov_27sem404wr().s[122]++;
      segments.push(segment);
    }
    // Filtrer les segments vides et fusionner les segments adjacents identiques
    /* istanbul ignore next */
    cov_27sem404wr().s[123]++;
    return this.mergeAdjacentSegments(segments.filter(s => {
      /* istanbul ignore next */
      cov_27sem404wr().f[18]++;
      cov_27sem404wr().s[124]++;
      return /* istanbul ignore next */(cov_27sem404wr().b[24][0]++, s.content.trim()) ||
      /* istanbul ignore next */
      (cov_27sem404wr().b[24][1]++, s.type === 'equation');
    }));
  }
  mergeAdjacentSegments(segments) {
    /* istanbul ignore next */
    cov_27sem404wr().f[19]++;
    cov_27sem404wr().s[125]++;
    if (segments.length <= 1) {
      /* istanbul ignore next */
      cov_27sem404wr().b[25][0]++;
      cov_27sem404wr().s[126]++;
      return segments;
    } else
    /* istanbul ignore next */
    {
      cov_27sem404wr().b[25][1]++;
    }
    const merged =
    /* istanbul ignore next */
    (cov_27sem404wr().s[127]++, []);
    let current =
    /* istanbul ignore next */
    (cov_27sem404wr().s[128]++, {
      ...segments[0]
    });
    /* istanbul ignore next */
    cov_27sem404wr().s[129]++;
    for (let i =
    /* istanbul ignore next */
    (cov_27sem404wr().s[130]++, 1); i < segments.length; i++) {
      const next =
      /* istanbul ignore next */
      (cov_27sem404wr().s[131]++, segments[i]);
      // Vrifier si les segments peuvent tre fusionns
      /* istanbul ignore next */
      cov_27sem404wr().s[132]++;
      if (this.canMergeSegments(current, next)) {
        /* istanbul ignore next */
        cov_27sem404wr().b[26][0]++;
        cov_27sem404wr().s[133]++;
        current.content += next.content;
        /* istanbul ignore next */
        cov_27sem404wr().s[134]++;
        current.end = next.end;
      } else {
        /* istanbul ignore next */
        cov_27sem404wr().b[26][1]++;
        cov_27sem404wr().s[135]++;
        merged.push(current);
        /* istanbul ignore next */
        cov_27sem404wr().s[136]++;
        current = {
          ...next
        };
      }
    }
    /* istanbul ignore next */
    cov_27sem404wr().s[137]++;
    merged.push(current);
    /* istanbul ignore next */
    cov_27sem404wr().s[138]++;
    return merged;
  }
  canMergeSegments(a, b) {
    /* istanbul ignore next */
    cov_27sem404wr().f[20]++;
    cov_27sem404wr().s[139]++;
    if (a.type !== b.type) {
      /* istanbul ignore next */
      cov_27sem404wr().b[27][0]++;
      cov_27sem404wr().s[140]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_27sem404wr().b[27][1]++;
    }
    cov_27sem404wr().s[141]++;
    if (a.type === 'equation') {
      /* istanbul ignore next */
      cov_27sem404wr().b[28][0]++;
      cov_27sem404wr().s[142]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_27sem404wr().b[28][1]++;
    } // Ne jamais fusionner les quations
    // Comparer toutes les proprits de formatage
    cov_27sem404wr().s[143]++;
    return /* istanbul ignore next */(cov_27sem404wr().b[29][0]++, a.bold === b.bold) &&
    /* istanbul ignore next */
    (cov_27sem404wr().b[29][1]++, a.italic === b.italic) &&
    /* istanbul ignore next */
    (cov_27sem404wr().b[29][2]++, a.code === b.code) &&
    /* istanbul ignore next */
    (cov_27sem404wr().b[29][3]++, a.strikethrough === b.strikethrough) &&
    /* istanbul ignore next */
    (cov_27sem404wr().b[29][4]++, a.underline === b.underline) &&
    /* istanbul ignore next */
    (cov_27sem404wr().b[29][5]++, JSON.stringify(a.link) === JSON.stringify(b.link));
  }
  /**
   * Convertit du texte simple en rich text
   */
  createSimpleRichText(content) {
    /* istanbul ignore next */
    cov_27sem404wr().f[21]++;
    cov_27sem404wr().s[144]++;
    return [{
      type: 'text',
      text: {
        content
      }
    }];
  }
  /**
   * Cre un rich text avec formatage spcifique
   */
  createFormattedRichText(content, formatting =
  /* istanbul ignore next */
  (cov_27sem404wr().b[30][0]++, {})) {
    /* istanbul ignore next */
    cov_27sem404wr().f[22]++;
    cov_27sem404wr().s[145]++;
    return [{
      type: 'text',
      text: {
        content
      },
      annotations: formatting
    }];
  }
  /**
   * Cre un rich text avec lien
   */
  createLinkRichText(content, url) {
    /* istanbul ignore next */
    cov_27sem404wr().f[23]++;
    cov_27sem404wr().s[146]++;
    return [{
      type: 'text',
      text: {
        content,
        link: {
          url
        }
      }
    }];
  }
  /**
   * Cre un rich text quation
   */
  createEquationRichText(expression) {
    /* istanbul ignore next */
    cov_27sem404wr().f[24]++;
    cov_27sem404wr().s[147]++;
    return [{
      type: 'equation',
      equation: {
        expression
      }
    }];
  }
  /**
   * Combine plusieurs rich text arrays
   */
  combineRichText(...richTexts) {
    /* istanbul ignore next */
    cov_27sem404wr().f[25]++;
    cov_27sem404wr().s[148]++;
    return richTexts.flat();
  }
  /**
   * Tronque le rich text si ncessaire
   */
  truncateRichText(richText, maxLength) {
    /* istanbul ignore next */
    cov_27sem404wr().f[26]++;
    let currentLength =
    /* istanbul ignore next */
    (cov_27sem404wr().s[149]++, 0);
    const result =
    /* istanbul ignore next */
    (cov_27sem404wr().s[150]++, []);
    /* istanbul ignore next */
    cov_27sem404wr().s[151]++;
    for (const item of richText) {
      const content =
      /* istanbul ignore next */
      (cov_27sem404wr().s[152]++, item.type === 'text' ?
      /* istanbul ignore next */
      (cov_27sem404wr().b[31][0]++,
      /* istanbul ignore next */
      (cov_27sem404wr().b[32][0]++, item.text?.content) ||
      /* istanbul ignore next */
      (cov_27sem404wr().b[32][1]++, '')) :
      /* istanbul ignore next */
      (cov_27sem404wr().b[31][1]++, item.type === 'equation' ?
      /* istanbul ignore next */
      (cov_27sem404wr().b[33][0]++,
      /* istanbul ignore next */
      (cov_27sem404wr().b[34][0]++, item.equation?.expression) ||
      /* istanbul ignore next */
      (cov_27sem404wr().b[34][1]++, '')) :
      /* istanbul ignore next */
      (cov_27sem404wr().b[33][1]++, '')));
      /* istanbul ignore next */
      cov_27sem404wr().s[153]++;
      if (currentLength + content.length <= maxLength) {
        /* istanbul ignore next */
        cov_27sem404wr().b[35][0]++;
        cov_27sem404wr().s[154]++;
        result.push(item);
        /* istanbul ignore next */
        cov_27sem404wr().s[155]++;
        currentLength += content.length;
      } else {
        /* istanbul ignore next */
        cov_27sem404wr().b[35][1]++;
        const remainingLength =
        /* istanbul ignore next */
        (cov_27sem404wr().s[156]++, maxLength - currentLength);
        /* istanbul ignore next */
        cov_27sem404wr().s[157]++;
        if (remainingLength > 10) {
          /* istanbul ignore next */
          cov_27sem404wr().b[36][0]++;
          const truncatedContent =
          /* istanbul ignore next */
          (cov_27sem404wr().s[158]++, content.substring(0, remainingLength - 10) + '...');
          /* istanbul ignore next */
          cov_27sem404wr().s[159]++;
          if (item.type === 'text') {
            /* istanbul ignore next */
            cov_27sem404wr().b[37][0]++;
            cov_27sem404wr().s[160]++;
            result.push({
              ...item,
              text: {
                ...item.text,
                content: truncatedContent
              }
            });
          } else {
            /* istanbul ignore next */
            cov_27sem404wr().b[37][1]++;
            cov_27sem404wr().s[161]++;
            if (item.type === 'equation') {
              /* istanbul ignore next */
              cov_27sem404wr().b[38][0]++;
              cov_27sem404wr().s[162]++;
              result.push({
                type: 'text',
                text: {
                  content: truncatedContent
                }
              });
            } else
            /* istanbul ignore next */
            {
              cov_27sem404wr().b[38][1]++;
            }
          }
        } else
        /* istanbul ignore next */
        {
          cov_27sem404wr().b[36][1]++;
        }
        cov_27sem404wr().s[163]++;
        break;
      }
    }
    /* istanbul ignore next */
    cov_27sem404wr().s[164]++;
    return result;
  }
}
/* istanbul ignore next */
cov_27sem404wr().s[165]++;
exports.RichTextConverter = RichTextConverter;
