{
  "name": "@notion-clipper/core",
  "version": "1.0.0",
  "description": "Core business logic and abstractions for Notion Clipper",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc --noEmit false || echo 'Build completed with warnings'",
    "build:force": "tsc --skipLibCheck --noEmit false",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "test": "jest"
  },
  "exports": {
    ".": "./dist/index.js",
    "./parsers": "./dist/parsers/index.js",
    "./converters": "./dist/converters/index.js",
    "./services": "./dist/services/index.js",
    "./interfaces": "./dist/interfaces/index.js",
    "./types": "./dist/types/index.js"
  },
  "dependencies": {
    "marked": "^16.1.1",
    "node-html-parser": "^7.0.1",
    "crypto-js": "^4.2.0",
    "natural": "^6.10.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/crypto-js": "^4.2.0",
    "@types/natural": "^6.0.0",
    "typescript": "^5.0.0",
    "rimraf": "^5.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0"
  },
  "author": "Rayane Ben Haga",
  "license": "MIT"
}
